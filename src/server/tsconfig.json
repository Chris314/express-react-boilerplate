{
    "files": [
        "server.ts"
    ],
    "compilerOptions": {
        "rootDir": "./",
        "outDir": "../../dist/",
        "noImplicitAny": true,
        "esModuleInterop": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es6",
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "lib": [
            "es2017"
        ],
        "types": [
            "node",
            "express"
        ]
    }
}